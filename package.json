{
  "name": "reinspect",
  "version": "1.1.1",
  "description": "Use redux devtools to inspect useState and useReducer hooks",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/reinspect.esm.js",
  "typings": "types/index.d.ts",
  "repository": "git@github.com:troch/reinspect.git",
  "author": "Thomas Roch <thomas.c.roch@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint src tests"
  },
  "dependencies": {
    "@redux-devtools/extension": "^3.2.2",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/enzyme": "~3.9.1",
    "@types/enzyme-adapter-react-16": "~1.0.5",
    "@types/jest": "~24.0.11",
    "@types/react": "~16.8.3",
    "conventional-changelog-cli": "~2.0.12",
    "enzyme": "~3.9.0",
    "enzyme-adapter-react-16": "~1.12.1",
    "husky": "^4.2.3",
    "patch-package": "^6.4.7",
    "react": "~16.8.6",
    "react-dom": "~16.8.6",
    "react-test-renderer": "~16.8.2",
    "tsdx": "0.14.1",
    "typescript": "~3.8.3"
  },
  "peerDependencies": {
    "react": ">=16.8.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/tests/test.setup.js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tests/tsconfig.json"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src tests"
    }
  }
}
